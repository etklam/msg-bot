    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.16.2",
    "next": "14.2.5",
    "openai": "^4.52.7",
    "react": "^18",
    "react-dom": "^18",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "jest": "^29.7.0",
    "prisma": "^5.16.2",
    "ts-jest": "^29.2.2",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
